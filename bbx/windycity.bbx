\ExecuteBibliographyOptions{%
  abbreviate=true,
  autocite=footnote,
  autopunct=true,
  block=none,
  citetracker=context,
  date=long,
  dateabbrev=false,
  dateusetime=true,
  doi=true,
  eprint=true,
  ibidtracker=constrict,
  idemtracker=false,
  indexing=true,
  labeldateparts=true,
  loccittracker=constrict,
  minbibnames=7,
  maxbibnames=10,
  mincitenames=1,
  maxcitenames=3,
% Remember, if 'mincrossrefs' is greater than 1, and you cite just one
% entrykey in a document (or refsection, etc), the field 'crossref' is
% undefined.
  mincrossrefs=2,
  minxrefs=2,
  pagetracker=page,
  parentracker=true,
  sortcites=false,
  sorting=nty,
  time=12h,
  timezones=true,
  uniquelist=minyear,
  uniquename=minfull,
  url=true,
  urldate=long,
